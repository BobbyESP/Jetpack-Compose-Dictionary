<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devaloteam.dictionary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".DictionaryApp"
        android:allowBackup="true"
        android:icon="@drawable/baseline_book_red_900_vector"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_book_red_900_vector"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dictionary">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Dictionary.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>